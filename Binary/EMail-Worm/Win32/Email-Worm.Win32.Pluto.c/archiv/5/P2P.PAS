FUNCTION XoloxShare : STRING;
VAR
  I : WORD;
  S : STRING;
BEGIN
  Reg.RootKey:=HKEY_CURRENT_USER;
  Reg.OpenKey('Software\Xolox',False);
  S:=Reg.ReadString('shareddirs');
