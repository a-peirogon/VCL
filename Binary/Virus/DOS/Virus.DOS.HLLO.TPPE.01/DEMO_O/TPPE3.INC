procedure TPPE_3;
var i,j:integer;
    t:text;
begin
{$IFOPT S-}j:=1;{$ENDIF}
assign(t,'tppes.inc');
rewrite(t);
write(t,'(');
{$IFOPT S+}j:=1;{$ENDIF}
for i:=1 to ComLen do
  begin
  write(t,Dropper[i] xor NewKey[j]);
  if i<>ComLen then write(t,',');
  {$IFOPT T-}if (i mod 20)=0 then writeln(t,'');{$ENDIF}
  if j=40 then j:=1 else {$IFOPT V+}inc(j);{$ELSE}j:=j+1;{$ENDIF}
  {$IFOPT T+}if (i mod 20)=0 then writeln(t,'');{$ENDIF}
  end;
write(t,')');
close(t);
end;