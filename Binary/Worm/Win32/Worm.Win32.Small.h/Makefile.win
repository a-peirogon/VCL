   # Project: Endor
   # Makefile created by Dev-C++ 4.9.8.0

   CPP  = g++.exe
   CC   = gcc.exe
   WINDRES = windres.exe
   RES  = endor_private.res
   OBJ  = endor.o $(RES)
   LINKOBJ  = endor.o $(RES)
   LIBS =  -L"C:/Dev-Cpp/lib" -mwindows
   INCS =  -I"C:/Dev-Cpp/include"
   CXXINCS =  -I"C:/Dev-Cpp/include/c++"  -I"C:/Dev-Cpp/include/c++/mingw32"  -I"C:/Dev-Cpp/include/c++/backward"  -I"C:/Dev-Cpp/include"
   BIN  = endor.exe
   CXXFLAGS = $(CXXINCS)  -fexpensive-optimizations -O3 -lobjc
   CFLAGS = $(INCS)  -fexpensive-optimizations -O3 -lobjc

   .PHONY: all all-before all-after clean clean-custom

   all: all-before endor.exe all-after


   clean: clean-custom
       rm -f $(OBJ) $(BIN)

   $(BIN): $(LINKOBJ)
       $(CPP) $(LINKOBJ) -o "endor.exe" $(LIBS)

   endor.o: endor.cpp
       $(CPP) -c endor.cpp -o endor.o $(CXXFLAGS)

   endor_private.res: endor_private.rc
       $(WINDRES) -i endor_private.rc -I rc -o endor_private.res -O coff
