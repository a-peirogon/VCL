                       scriptIndex+=marker.length
                       state=STATE_READLEN
                   }else{
                       stringIndex=stringIndex==0?0:stringIndex
                       unEncodingString+=encodingString.substr(stringIndex,encodingString.length)
                       state=0
                   }
               break
           case (STATE_READLEN):
